{
    "name": "maryushenina/ai-client-sdk",
    "description": "Lightweight SDK for using OpenAI Chat Completion API in PHP projects.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MaryUshenina\\AiClientSdk\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload-dev": {
        "psr-4": {
            "MaryUshenina\\AiClientSdk\\Tests\\": "tests/"
        }
    }
}
